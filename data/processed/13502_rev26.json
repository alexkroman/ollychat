[
  {
    "name": "Uptime",
    "description": "",
    "queries": [
      "time() - max(minio_node_process_starttime_seconds{job=~\"$scrape_jobs\"})"
    ]
  },
  {
    "name": "Total S3 Ingress",
    "description": "",
    "queries": [
      "sum by (instance) (minio_s3_traffic_received_bytes{job=~\"$scrape_jobs\"})"
    ]
  },
  {
    "name": "Capacity",
    "description": "",
    "queries": [
      "topk(1, sum(minio_cluster_capacity_usable_total_bytes{job=~\"$scrape_jobs\"}) by (instance)) - topk(1, sum(minio_cluster_capacity_usable_free_bytes{job=~\"$scrape_jobs\"}) by (instance))",
      "topk(1, sum(minio_cluster_capacity_usable_free_bytes{job=~\"$scrape_jobs\"}) by (instance)) "
    ]
  },
  {
    "name": "Data Usage Growth",
    "description": "",
    "queries": [
      "max(minio_cluster_usage_total_bytes{job=~\"$scrape_jobs\"})"
    ]
  },
  {
    "name": "Object Size Distribution",
    "description": "",
    "queries": [
      "minio_cluster_objects_size_distribution{job=~\"$scrape_jobs\"}"
    ]
  },
  {
    "name": "Open FDs ",
    "description": "",
    "queries": [
      "minio_node_file_descriptor_open_total{job=~\"$scrape_jobs\"}"
    ]
  },
  {
    "name": "Total S3 Egress",
    "description": "",
    "queries": [
      "sum by (instance) (minio_s3_traffic_sent_bytes{job=~\"$scrape_jobs\"})"
    ]
  },
  {
    "name": "Goroutines",
    "description": "",
    "queries": [
      "minio_node_go_routine_total{job=~\"$scrape_jobs\"}"
    ]
  },
  {
    "name": "Cluster Health Status",
    "description": "",
    "queries": [
      "minio_cluster_health_status{job=~\"$scrape_jobs\"}"
    ]
  },
  {
    "name": "Total Online/Offline Drives",
    "description": "",
    "queries": [
      "max(minio_cluster_drive_online_total{job=~\"$scrape_jobs\"})",
      "max(minio_cluster_drive_offline_total{job=~\"$scrape_jobs\"})"
    ]
  },
  {
    "name": "Number of Buckets",
    "description": "",
    "queries": [
      "max(minio_cluster_bucket_total{job=~\"$scrape_jobs\"})"
    ]
  },
  {
    "name": "S3 API Ingress Rate ",
    "description": "",
    "queries": [
      "sum by (server) (rate(minio_s3_traffic_received_bytes{job=~\"$scrape_jobs\"}[30m]))"
    ]
  },
  {
    "name": "S3 API Egress Rate ",
    "description": "",
    "queries": [
      "sum by (server) (rate(minio_s3_traffic_sent_bytes{job=~\"$scrape_jobs\"}[30m]))"
    ]
  },
  {
    "name": "Total Online Servers",
    "description": "",
    "queries": [
      "max(minio_cluster_nodes_online_total{job=~\"$scrape_jobs\"})"
    ]
  },
  {
    "name": "Number of Objects",
    "description": "",
    "queries": [
      "max(minio_cluster_usage_object_total{job=~\"$scrape_jobs\"})"
    ]
  },
  {
    "name": "Time Since Last Heal",
    "description": "",
    "queries": [
      "max(minio_heal_time_last_activity_nano_seconds{job=~\"$scrape_jobs\"})"
    ]
  },
  {
    "name": "Time Since Last Scan",
    "description": "",
    "queries": [
      "max(minio_usage_last_activity_nano_seconds{job=~\"$scrape_jobs\"})"
    ]
  },
  {
    "name": "S3 API Request Rate",
    "description": "",
    "queries": [
      "sum by (server,api) (increase(minio_s3_requests_total{job=~\"$scrape_jobs\"}[30m]))"
    ]
  },
  {
    "name": "S3 API Request Error Rate (4xx)",
    "description": "",
    "queries": [
      "sum by (server,api) (increase(minio_s3_requests_4xx_errors_total{job=~\"$scrape_jobs\"}[30m]))"
    ]
  },
  {
    "name": "S3 API Request Error Rate (5xx)",
    "description": "",
    "queries": [
      "sum by (server,api) (increase(minio_s3_requests_5xx_errors_total{job=~\"$scrape_jobs\"}[30m]))"
    ]
  },
  {
    "name": "Health Breakdown",
    "description": "",
    "queries": [
      "minio_cluster_health_erasure_set_online_drives{job=~\"$scrape_jobs\"}",
      "minio_cluster_health_erasure_set_read_quorum{job=~\"$scrape_jobs\"}",
      "minio_cluster_health_erasure_set_write_quorum{job=~\"$scrape_jobs\"}",
      "minio_cluster_health_erasure_set_healing_drives{job=~\"$scrape_jobs\"}",
      "minio_cluster_health_erasure_set_status{job=~\"$scrape_jobs\"}"
    ]
  },
  {
    "name": "Memory Usage ",
    "description": "",
    "queries": [
      "minio_node_process_resident_memory_bytes{job=~\"$scrape_jobs\"}"
    ]
  },
  {
    "name": "Read, Write I/O",
    "description": "",
    "queries": [
      "rate(minio_node_io_rchar_bytes{job=~\"$scrape_jobs\"}[30m])",
      "rate(minio_node_io_wchar_bytes{job=~\"$scrape_jobs\"}[30m])"
    ]
  },
  {
    "name": "CPU Usage",
    "description": "",
    "queries": [
      "rate(minio_node_process_cpu_total_seconds{job=~\"$scrape_jobs\"}[30m])"
    ]
  },
  {
    "name": "Internode Traffic",
    "description": "Total number of bytes received and sent on MinIO cluster",
    "queries": [
      "rate(minio_inter_node_traffic_sent_bytes{job=~\"$scrape_jobs\"}[30m])",
      "rate(minio_inter_node_traffic_received_bytes{job=~\"$scrape_jobs\"}[30m])"
    ]
  },
  {
    "name": "File Descriptors",
    "description": "",
    "queries": [
      "minio_node_file_descriptor_open_total{job=~\"$scrape_jobs\"}"
    ]
  },
  {
    "name": "Syscalls",
    "description": "Number of online drives per MinIO Server",
    "queries": [
      "rate(minio_node_syscall_read_total{job=~\"$scrape_jobs\"}[30m])",
      "rate(minio_node_syscall_write_total{job=~\"$scrape_jobs\"}[30m])"
    ]
  },
  {
    "name": "Scanned Objects",
    "description": "",
    "queries": [
      "rate(minio_node_scanner_objects_scanned{job=~\"$scrape_jobs\"}[30m])"
    ]
  },
  {
    "name": "Scanned Versions",
    "description": "",
    "queries": [
      "rate(minio_node_scanner_versions_scanned{job=~\"$scrape_jobs\"}[30m])"
    ]
  },
  {
    "name": "Scanned Directories",
    "description": "",
    "queries": [
      "rate(minio_node_scanner_directories_scanned{job=~\"$scrape_jobs\"}[30m])"
    ]
  },
  {
    "name": "KMS Uptime",
    "description": "",
    "queries": [
      "minio_cluster_kms_uptime{job=~\"$scrape_jobs\"}"
    ]
  },
  {
    "name": "KMS Request 4xx Error Rate",
    "description": "",
    "queries": [
      "sum by (server) (increase(minio_cluster_kms_request_error{job=~\"$scrape_jobs\"}[30m]))"
    ]
  },
  {
    "name": "KMS Online(1)/Offline(0)",
    "description": "",
    "queries": [
      "sum by (server) (minio_cluster_kms_online{job=~\"$scrape_jobs\"})"
    ]
  },
  {
    "name": "Bucket Scans Finished",
    "description": "",
    "queries": [
      "rate(minio_node_scanner_bucket_scans_finished{job=~\"$scrape_jobs\"}[30m])"
    ]
  },
  {
    "name": "KMS Request 5xx Error Rate",
    "description": "",
    "queries": [
      "sum by (server) (increase(minio_cluster_kms_request_failure{job=~\"$scrape_jobs\"}[30m]))"
    ]
  },
  {
    "name": "KMS Request Success Rate ",
    "description": "",
    "queries": [
      "sum by (server) (rate(minio_cluster_kms_request_success{job=~\"$scrape_jobs\"}[30m]))"
    ]
  },
  {
    "name": "Bucket Scans Started",
    "description": "",
    "queries": [
      "rate(minio_node_scanner_bucket_scans_started{job=~\"$scrape_jobs\"}[30m])"
    ]
  }
]