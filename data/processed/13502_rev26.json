[
  {
    "name": "Uptime",
    "description": "",
    "queries": [
      "time() - max(minio_node_process_starttime_seconds)"
    ]
  },
  {
    "name": "Total S3 Ingress",
    "description": "",
    "queries": [
      "sum by (instance) (minio_s3_traffic_received_bytes)"
    ]
  },
  {
    "name": "Capacity",
    "description": "",
    "queries": [
      "topk(1, sum(minio_cluster_capacity_usable_total_bytes) by (instance)) - topk(1, sum(minio_cluster_capacity_usable_free_bytes) by (instance))",
      "topk(1, sum(minio_cluster_capacity_usable_free_bytes) by (instance)) "
    ]
  },
  {
    "name": "Data Usage Growth",
    "description": "",
    "queries": [
      "max(minio_cluster_usage_total_bytes)"
    ]
  },
  {
    "name": "Object Size Distribution",
    "description": "",
    "queries": [
      "minio_cluster_objects_size_distribution"
    ]
  },
  {
    "name": "Open FDs ",
    "description": "",
    "queries": [
      "minio_node_file_descriptor_open_total"
    ]
  },
  {
    "name": "Total S3 Egress",
    "description": "",
    "queries": [
      "sum by (instance) (minio_s3_traffic_sent_bytes)"
    ]
  },
  {
    "name": "Goroutines",
    "description": "",
    "queries": [
      "minio_node_go_routine_total"
    ]
  },
  {
    "name": "Cluster Health Status",
    "description": "",
