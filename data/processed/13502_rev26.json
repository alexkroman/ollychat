    ]
  },
  {
    "name": "Total Online/Offline Drives",
    "description": "",
    "queries": [
      "max(minio_cluster_drive_online_total)",
      "max(minio_cluster_drive_offline_total)"
    ]
  },
  {
    "name": "Number of Buckets",
    "description": "",
    "queries": [
      "max(minio_cluster_bucket_total)"
    ]
  },
  {
    "name": "S3 API Ingress Rate ",
    "description": "",
    "queries": [
      "sum by (server) (rate(minio_s3_traffic_received_bytes[30m]))"
    ]
  },
  {
    "name": "S3 API Egress Rate ",
    "description": "",
    "queries": [
      "sum by (server) (rate(minio_s3_traffic_sent_bytes[30m]))"
    ]
  },
  {
    "name": "Total Online Servers",
    "description": "",
    "queries": [
      "max(minio_cluster_nodes_online_total)"
    ]
  },
  {
    "name": "Number of Objects",
    "description": "",
