    ]
  },
  {
    "name": "S3 API Request Rate",
    "description": "",
    "queries": [
      "sum by (server,api) (increase(minio_s3_requests_total[30m]))"
    ]
  },
  {
    "name": "S3 API Request Error Rate (4xx)",
    "description": "",
    "queries": [
      "sum by (server,api) (increase(minio_s3_requests_4xx_errors_total[30m]))"
