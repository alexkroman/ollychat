[
  {
    "name": "Process CPU Usage",
    "description": "",
    "queries": [
      "irate(process_cpu_user_seconds_total{instance=~\"$instance\"}[2m]) * 100",
      "irate(process_cpu_system_seconds_total{instance=~\"$instance\"}[2m]) * 100"
    ]
  },
  {
    "name": "Event Loop Lag",
    "description": "",
    "queries": [
      "nodejs_eventloop_lag_seconds{instance=~\"$instance\"}"
    ]
  },
  {
    "name": "Node.js Version",
    "description": "",
    "queries": [
      "sum(nodejs_version_info{instance=~\"$instance\"}) by (version)"
    ]
  },
  {
    "name": "Process Restart Times",
    "description": "",
    "queries": [
      "sum(changes(process_start_time_seconds{instance=~\"$instance\"}[1m]))"
    ]
  },
  {
    "name": "Process Memory Usage",
    "description": "",
    "queries": [
      "process_resident_memory_bytes{instance=~\"$instance\"}",
      "nodejs_heap_size_total_bytes{instance=~\"$instance\"}",
      "nodejs_heap_size_used_bytes{instance=~\"$instance\"}",
      "nodejs_external_memory_bytes{instance=~\"$instance\"}"
    ]
  },
  {
    "name": "Active Handlers/Requests Total",
    "description": "",
    "queries": [
      "nodejs_active_handles_total{instance=~\"$instance\"}",
      "nodejs_active_requests_total{instance=~\"$instance\"}"
    ]
  },
  {
    "name": "Heap Total Detail",
    "description": "",
    "queries": [
      "nodejs_heap_space_size_total_bytes{instance=~\"$instance\"}"
    ]
  },
  {
    "name": "Heap Used Detail",
    "description": "",
    "queries": [
      "nodejs_heap_space_size_used_bytes{instance=~\"$instance\"}"
    ]
  },
  {
    "name": "Heap Available Detail",
    "description": "",
    "queries": [
      "nodejs_heap_space_size_available_bytes{instance=~\"$instance\"}"
    ]
  }
]