[
  {
    "name": "Project $Namespace Quotas - CPU Requests & Limits",
    "description": "",
    "queries": [
      "sum (kube_resourcequota) * 1000",
      "sum (kube_resourcequota) * 1000",
      "sum (kube_resourcequota) * 1000"
    ]
  },
  {
    "name": "Pods CPU usage for $Namespace",
    "description": "",
    "queries": [
      "sum (rate (container_cpu_usage_seconds_total[2m]))"
    ]
  },
  {
    "name": "Project $Namespace Quotas - Memory Requests & Limits",
    "description": "",
    "queries": [
      "sum (kube_resourcequota) ",
      "sum (kube_resourcequota) ",
      "sum (kube_resourcequota) "
    ]
  },
  {
    "name": "Total Pods memory usage for $Namespace",
    "description": "",
    "queries": [
      "sum (container_memory_working_set_bytes)"
    ]
  },
  {
    "name": "Total Network I/O pressure for  $Namespace",
    "description": "",
    "queries": [
      "sum (rate (container_network_receive_bytes_total[2m]))",
      "- sum (rate (container_network_transmit_bytes_total[2m]))"
    ]
  }
]