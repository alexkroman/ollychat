[
  {
    "name": "Total Nodes",
    "description": "",
    "queries": [
      "round(\n  sum(\n    cluster_autoscaler_nodes_count\n  )\n)\n"
    ]
  },
  {
    "name": "Max Nodes",
    "description": "",
    "queries": [
      "round(\n  sum(\n    cluster_autoscaler_max_nodes_count\n  )\n)\n"
    ]
  },
  {
    "name": "Node Groups",
    "description": "",
    "queries": [
      "round(\n  sum(\n    cluster_autoscaler_node_groups_count\n  )\n)\n"
    ]
  },
  {
    "name": "Healthy Nodes",
    "description": "",
    "queries": [
      "round(\n  sum(\n    cluster_autoscaler_nodes_count\n  ) /\n  sum(\n    cluster_autoscaler_nodes_count\n  ) * 100\n)\n"
    ]
  },
  {
    "name": "Safe To Scale",
    "description": "",
    "queries": [
      "round(\n  sum(\n    cluster_autoscaler_cluster_safe_to_autoscale\n  )\n)\n"
    ]
  },
  {
    "name": "Unscheduled Pods",
    "description": "",
    "queries": [
      "round(\n  sum(\n    cluster_autoscaler_unschedulable_pods_count\n  )\n)\n"
    ]
  },
  {
    "name": "Last Scale Down",
    "description": "",
    "queries": [
      "time() - sum(\n  cluster_autoscaler_last_activity\n)\n"
    ]
  },
  {
    "name": "Last Scale Up",
    "description": "",
    "queries": [
      "time() - sum(\n  cluster_autoscaler_last_activity\n)\n"
    ]
  },
  {
    "name": "Pod Activity",
    "description": "",
    "queries": [
      "round(\n  sum(\n    increase(\n      cluster_autoscaler_unschedulable_pods_count[2m]\n    )\n  ) by (type)\n)\n",
      "round(\n  sum(\n    increase(\n      cluster_autoscaler_evicted_pods_total[2m]\n    )\n  )\n)\n"
    ]
  },
  {
    "name": "Node Activity",
    "description": "",
    "queries": [
      "round(\n  sum(\n    cluster_autoscaler_nodes_count\n  ) by (state)\n)\n"
    ]
  },
  {
    "name": "Autoscaling Activity",
    "description": "",
    "queries": [
      "round(\n  sum(\n    cluster_autoscaler_nodes_count\n  )\n)\n",
      "round(\n  sum(\n    cluster_autoscaler_unneeded_nodes_count\n  )\n)\n",
      "round(\n  sum(\n    increase(\n      cluster_autoscaler_scaled_up_nodes_total[2m]\n    )\n  )\n)\n",
      "round(\n  sum(\n    increase(\n      cluster_autoscaler_scaled_down_nodes_total[2m]\n    )\n  )\n)\n"
    ]
  }
]