[
  {
    "name": "Memory Usage",
    "description": "Memory usage of each running instance",
    "query": "sum by (pod) (\n  container_memory_working_set_bytes\n)",
    "metrics": [
      "container_memory_working_set_bytes"
    ]
  },
  {
    "name": "Memory Usage",
    "description": "Memory usage of each running instance",
    "query": "sum by (pod) (\n  go_memstats_stack_inuse_bytes\n)",
    "metrics": [
      "go_memstats_stack_inuse_bytes"
    ]
  },
  {
    "name": "Memory Usage",
    "description": "Memory usage of each running instance",
    "query": "sum by (pod) (\n  go_memstats_heap_inuse_bytes\n)",
    "metrics": [
      "go_memstats_heap_inuse_bytes"
    ]
  },
  {
    "name": "Memory Usage",
    "description": "Memory usage of each running instance",
