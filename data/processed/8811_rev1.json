[
  {
    "name": "Actor System Processed messages",
    "description": "Processed messages by actor system",
    "queries": [
      "sum by(job, system)(rate(akka_system_dead_letters_total[2m]))",
      "sum by(job, system) (rate(akka_system_unhandled_messages_total[2m]))",
      "sum by(job, system) (rate(akka_system_processed_messages_total[2m]))"
    ]
  },
  {
    "name": "Dead Letters queue",
    "description": "",
    "queries": [
      "sum by (job, system)(akka_system_dead_letters_total)"
    ]
  },
  {
    "name": "Unhanded messages",
    "description": "",
    "queries": [
      "sum by (job, system)(akka_system_unhandled_messages_total)"
    ]
  },
  {
    "name": "Avg Active Actors",
    "description": "Average count of actors in the system",
    "queries": [
      "akka_system_active_actors_sum/akka_system_active_actors_count"
    ]
  },
  {
    "name": "Threads count",
    "description": "A number of threads used",
    "queries": [
      "executor_threads_sum/executor_threads_count",
      "max(executor_pool)",
      "executor_threads_sum/executor_threads_count"
    ]
  },
  {
    "name": "Executor Queue size histogram",
    "description": "",
    "queries": [
      "sum by(le)(increase(executor_queue_bucket[2m]))"
    ]
  },
  {
    "name": "Executor tasks",
    "description": "Tracks the number of tasks processed by the executor service",
    "queries": [
      "executor_tasks_total"
    ]
  }
]