[
  {
    "name": "$Percentile 'th Response Time",
    "description": "",
    "queries": [
      "histogram_quantile(0.$Percentile, sum(rate(request_with_phases_duration_seconds_bucket[60s])) by (le, status_code, request_name, phase))"
    ]
  },
  {
    "name": "HTTP Stats",
    "description": "",
    "queries": [
      "sum(rate(request_with_phases_duration_seconds_count[1m])) by (request_name)",
      "sum(rate(request_with_phases_duration_seconds_count[1m])) by (request_name)",
      "histogram_quantile(0.5, sum(rate(request_with_phases_duration_seconds_bucket[1m])) by (le, request_name))",
      "histogram_quantile(0.9, sum(rate(request_with_phases_duration_seconds_bucket[1m])) by (le, request_name))",
      "histogram_quantile(0.99, sum(rate(request_with_phases_duration_seconds_bucket[1m])) by (le, request_name))",
      "sum(rate(request_with_phases_duration_seconds_count[1m])) by (request_name) / sum(rate(request_with_phases_duration_seconds_count[1m])) by (request_name)"
    ]
  },
  {
    "name": "Requests Per Second",
    "description": "",
    "queries": [
      "topk(10, sum(rate(request_duration_seconds_count[60s])) by (status_code, path))"
    ]
  },
  {
    "name": "Memory",
    "description": "",
    "queries": [
      "avg(runnerStats)"
    ]
  },
