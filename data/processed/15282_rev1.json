[
  {
    "name": "Cluster memory usage",
    "description": "",
    "queries": [
      "sum (container_memory_working_set_bytes) / sum (machine_memory_bytes) * 100"
    ]
  },
  {
    "name": "Cluster CPU usage (5m avg)",
    "description": "",
    "queries": [
      "sum (rate (container_cpu_usage_seconds_total[5m])) / sum (machine_cpu_cores) * 100"
    ]
  },
  {
    "name": "Cluster filesystem usage",
    "description": "",
    "queries": [
      "sum (container_fs_usage_bytes) / sum (container_fs_limit_bytes) * 100"
    ]
  },
  {
    "name": "Used",
    "description": "",
    "queries": [
      "container_memory_usage_bytes"
    ]
  },
  {
    "name": "Total",
    "description": "",
    "queries": [
      "(machine_memory_bytes)"
    ]
  },
  {
    "name": "Used",
    "description": "",
    "queries": [
      "sum (rate (container_cpu_usage_seconds_total[5m]))"
    ]
  },
  {
    "name": "Total",
    "description": "",
    "queries": [
      "machine_cpu_physical_cores"
    ]
  },
  {
    "name": "Used",
    "description": "",
