[
  {
    "name": "Cluster health",
    "description": "",
    "queries": [
      "elasticsearch_cluster_health_status==1 or (elasticsearch_cluster_health_status==1)+4 or (elasticsearch_cluster_health_status==1)+22"
    ]
  },
  {
    "name": "Tripped for breakers",
    "description": "",
    "queries": [
      "count(elasticsearch_breakers_tripped>0)"
    ]
  },
  {
    "name": "CPU usage Avg.",
    "description": "",
    "queries": [
      "sum (elasticsearch_process_cpu_percent ) / count (elasticsearch_process_cpu_percent )"
    ]
  },
  {
    "name": "JVM memory used Avg.",
    "description": "",
    "queries": [
      "sum (elasticsearch_jvm_memory_used_bytes) / sum (elasticsearch_jvm_memory_max_bytes) * 100"
    ]
  },
  {
    "name": "Nodes",
    "description": "Number of nodes in the cluster",
    "queries": [
      "elasticsearch_cluster_health_number_of_nodes"
    ]
  },
  {
    "name": "Data nodes",
    "description": "Number of data nodes in the cluster",
    "queries": [
      "elasticsearch_cluster_health_number_of_data_nodes"
    ]
  },
  {
    "name": "Pending tasks",
    "description": "Cluster level changes which have not yet been executed",
    "queries": [
      "elasticsearch_cluster_health_number_of_pending_tasks"
    ]
  },
  {
    "name": "Open file descriptors per cluster",
    "description": "",
    "queries": [
      "sum (elasticsearch_process_open_files_count)"
    ]
  },
  {
    "name": "Active primary shards",
    "description": "The number of primary shards in your cluster. This is an aggregate total across all indices.",
    "queries": [
      "elasticsearch_cluster_health_active_primary_shards"
    ]
  },
  {
    "name": "Active shards",
    "description": "Aggregate total of all shards across all indices, which includes replica shards",
    "queries": [
      "elasticsearch_cluster_health_active_shards"
    ]
  },
  {
    "name": "Initializing shards",
    "description": "Count of shards that are being freshly created",
    "queries": [
      "elasticsearch_cluster_health_initializing_shards"
    ]
  },
  {
    "name": "Relocating shards",
    "description": "The number of shards that are currently moving from one node to another node.",
    "queries": [
      "elasticsearch_cluster_health_relocating_shards"
    ]
  },
  {
    "name": "Delayed shards",
    "description": "Shards delayed to reduce reallocation overhead",
    "queries": [
      "elasticsearch_cluster_health_delayed_unassigned_shards "
    ]
  },
  {
    "name": "Unassigned shards",
    "description": "The number of shards that exist in the cluster state, but cannot be found in the cluster itself",
    "queries": [
      "elasticsearch_cluster_health_unassigned_shards "
    ]
  },
  {
    "name": "Load average",
    "description": "",
    "queries": [
      "elasticsearch_os_load1",
      "elasticsearch_os_load5",
      "elasticsearch_os_load15"
    ]
  },
  {
    "name": "CPU usage",
    "description": "",
    "queries": [
      "elasticsearch_process_cpu_percent"
    ]
  },
  {
    "name": "JVM memory usage",
    "description": "",
    "queries": [
      "elasticsearch_jvm_memory_used_bytes",
      "elasticsearch_jvm_memory_max_bytes",
      "elasticsearch_jvm_memory_pool_peak_used_bytes"
    ]
  },
  {
    "name": "JVM memory committed",
    "description": "",
    "queries": [
      "elasticsearch_jvm_memory_committed_bytes",
      "elasticsearch_jvm_memory_max_bytes"
    ]
  },
  {
    "name": "Documents count on node",
    "description": "",
    "queries": [
      "elasticsearch_indices_docs"
    ]
  },
  {
    "name": "Documents indexed rate",
    "description": "",
    "queries": [
      "irate(elasticsearch_indices_indexing_index_total[30m])"
    ]
  },
  {
    "name": "Documents deleted rate",
    "description": "Count of deleted documents on this node",
    "queries": [
      "irate(elasticsearch_indices_docs_deleted[30m])"
    ]
  },
  {
    "name": "Documents merged rate",
    "description": "",
    "queries": [
      "rate(elasticsearch_indices_merges_docs_total[30m])"
    ]
  },
  {
    "name": "Documents merged bytes",
    "description": "",
    "queries": [
      "irate(elasticsearch_indices_merges_total_size_bytes_total[30m])"
    ]
  }
]