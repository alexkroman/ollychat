[
  {
    "name": "Message in per second",
    "description": "",
    "queries": [
      "sum(rate(kafka_topic_partition_current_offset[1m])) by (topic)"
    ]
  },
  {
    "name": "Lag by  Consumer Group",
    "description": "",
    "queries": [
      "sum(kafka_consumergroup_lag) by (consumergroup, topic) "
    ]
  },
  {
    "name": "Message in per minute",
    "description": "",
    "queries": [
      "sum(delta(kafka_topic_partition_current_offset[5m])/5) by (topic)"
    ]
  },
  {
    "name": "Message consume per minute",
    "description": "",
    "queries": [
      "sum(delta(kafka_consumergroup_current_offset[5m])/5) by (consumergroup, topic)"
    ]
  },
  {
    "name": "Partitions per Topic",
    "description": "",
    "queries": [
      "sum by(topic) (kafka_topic_partitions)"
    ]
  }
]