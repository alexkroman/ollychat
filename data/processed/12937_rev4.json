  {
    "name": "Number of Pods",
    "description": "",
    "queries": [
      "count\n(\n  kube_pod_container_info\n) by (container)\n"
    ]
  },
  {
    "name": "CPU Core Usage vs. Requested",
    "description": "",
    "queries": [
      "sum(container_spec_cpu_shares) by (pod) / 1000",
