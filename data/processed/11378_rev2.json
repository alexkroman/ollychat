    "queries": [
      "process_uptime_seconds"
    ]
  },
  {
    "name": "Heap Used",
    "description": "",
    "queries": [
      "sum(jvm_memory_used_bytes)*100/sum(jvm_memory_max_bytes)"
    ]
  },
  {
    "name": "Non-Heap Used",
    "description": "",
    "queries": [
      "sum(jvm_memory_used_bytes)*100/sum(jvm_memory_max_bytes)"
    ]
  },
