  {
    "name": "Token Creation/Storage",
    "description": "",
    "queries": [
      "avg without(instance) (vault_token_create_count)",
      "avg without(instance) (vault_token_store_count)"
    ]
  },
  {
    "name": "Token Lookups",
    "description": "",
    "queries": [
      "avg(irate(vault_token_lookup_count[1m]))"
    ]
  },
  {
    "name": "Log Request Failures",
    "description": "",
    "queries": [
      "max(idelta(vault_audit_log_request_failure[1m]))"
    ]
  },
  {
    "name": "Log Requests",
    "description": "",
    "queries": [
      "avg(irate(vault_audit_log_request_count[1m]))",
