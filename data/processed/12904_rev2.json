[
  {
    "name": "Healthy Status",
    "description": "",
    "queries": [
      "up"
    ]
  },
  {
    "name": "Secrets",
    "description": "",
    "queries": [
      "avg without(instance) (vault_secret_kv_count)"
    ]
  },
  {
    "name": "Number of Identity Entities",
    "description": "",
    "queries": [
      "avg(vault_identity_num_entities)"
    ]
  },
  {
    "name": "Identity Entities Aliases by Method",
    "description": "",
    "queries": [
      "avg without(instance) (vault_identity_entity_alias_count)"
    ]
  },
  {
    "name": "Sealed Status",
    "description": "",
    "queries": [
      "max(1 + vault_core_unsealed)"
    ]
  },
  {
    "name": "Number of Leases",
    "description": "",
    "queries": [
      "avg(vault_expire_num_leases)"
    ]
  },
  {
