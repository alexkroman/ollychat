    "queries": [
      "avg(vault_token_count)"
    ]
  },
  {
    "name": "Tokens by Policy",
    "description": "",
    "queries": [
      "avg without(instance) (vault_token_count_by_policy)"
    ]
  },
  {
    "name": "Pending Tokens",
    "description": "",
    "queries": [
      "avg(vault_token_create_count - vault_token_store_count)"
    ]
  },
  {
    "name": "Tokens by TTL",
    "description": "",
    "queries": [
      "avg without(instance) (vault_token_count_by_ttl)"
    ]
  },
  {
    "name": "Tokens by Auth Method",
    "description": "",
    "queries": [
      "avg without(instance) (vault_token_count_by_auth)"
    ]
  },
  {
    "name": "Tokens Creation by Method & TTL",
    "description": "",
    "queries": [
      "avg by(auth_method, creation_ttl) (vault_token_creation)"
    ]
