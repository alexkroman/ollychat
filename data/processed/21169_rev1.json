[
  {
    "name": "Container Memory in Crossplane Namespace by Pod",
    "description": "",
    "queries": [
      "sum(container_memory_usage_bytes) by (container)"
    ]
  },
  {
    "name": "Container CPU in Crossplane Namespace by Pod",
    "description": "",
    "queries": [
      "sum(rate(container_cpu_usage_seconds_total[5m])) by (pod)"
    ]
  },
  {
    "name": "Reconciles by Controller (Top 10)",
    "description": "",
    "queries": [
      "topk(10, sum by (controller) (rate(controller_runtime_reconcile_total[5m])))"
    ]
  },
  {
    "name": "Workqueue Depth by Controller (Top 10)",
    "description": "",
    "queries": [
      "topk(10, sum by (name) (workqueue_depth))"
    ]
  },
  {
    "name": "Workqueue Duration",
    "description": "",
    "queries": [
      "sum(rate(workqueue_queue_duration_seconds_bucket[5m])) by (le)"
    ]
  },
  {
    "name": "Resource Time To Reconcile Average by Kind",
    "description": "",
    "queries": [
      "rate(upjet_resource_ttr_sum[5m])/rate(upjet_resource_ttr_count[5m])"
    ]
  },
  {
    "name": "Terraform CLI Time To Reconcile Average by Subcommand",
    "description": "",
    "queries": [
      "sum by (subcommand) (rate(upjet_terraform_cli_duration_sum[5m])/rate(upjet_terraform_cli_duration_count[5m]))"
    ]
  }
]