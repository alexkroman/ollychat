    ]
  },
  {
    "name": "Debug msg/sec",
    "description": "",
    "queries": [
      "sum by(level)(rate(log4j2_events_total[5m]))"
    ]
  },
  {
    "name": "Info msg/sec",
    "description": "",
    "queries": [
      "sum by(level)(rate(log4j2_events_total[5m]))"
    ]
  },
  {
    "name": "Warn msg/sec",
    "description": "",
    "queries": [
      "sum by(level)(rate(log4j2_events_total[5m]))"
    ]
  },
  {
    "name": "Error msg/sec",
    "description": "",
    "queries": [
      "sum by(level)(rate(log4j2_events_total[5m]))"
    ]
  },
  {
    "name": "Fatal msg/sec",
    "description": "",
    "queries": [
      "sum by(level)(rate(log4j2_events_total[5m]))"
    ]
  },
  {
    "name": "Messages received per transport",
    "description": "Messages on left axis, Spans on right axis",
    "queries": [
      "sum by(transport)(rate(zipkin_collector_messages_total[$__interval]))\n"
    ]
  },
  {
    "name": "Spans received per transport",
    "description": "Messages on left axis, Spans on right axis",
    "queries": [
      "sum by(transport)(rate(zipkin_collector_spans_total[$__interval]))\n"
    ]
  },
  {
    "name": "Dropped messages",
    "description": "The number of messages dropped (for kafka, scribe and http)",
    "queries": [
      "sum by(transport)(rate(zipkin_collector_messages_dropped_total[$__interval]))"
    ]
  },
  {
    "name": "Spans dropped",
    "description": "The number of spans dropped (for scribe, http and kafka)",
    "queries": [
      "sum by(transport)(rate(zipkin_collector_spans_dropped_total[$__interval]))\n"
    ]
  },
  {
    "name": "Bytes received per transport",
    "description": "",
    "queries": [
      "sum by (transport)(rate(zipkin_collector_bytes_total[$__interval]))"
    ]
  },
  {
    "name": "Requests per second",
    "description": "",
    "queries": [
      "rate(http_server_requests_seconds_count[$__interval])"
    ]
  },
  {
    "name": "GET Spans",
    "description": "",
    "queries": [
      "histogram_quantile(0.9, http_server_requests_seconds_bucket)",
      "histogram_quantile(0.5, http_server_requests_seconds_bucket)"
    ]
  },
  {
    "name": "POST Spans",
    "description": "",
    "queries": [
      "histogram_quantile(0.9, http_server_requests_seconds_bucket)",
      "histogram_quantile(0.5, http_server_requests_seconds_bucket)"
    ]
  },
  {
    "name": "System CPUs used, Load",
    "description": "",
    "queries": [
      "system_cpu_count",
      "system_load_average_1m"
    ]
  },
  {
    "name": "JVM Memory",
    "description": "",
    "queries": [
      "sum by(area,instance)(jvm_memory_used_bytes)",
      "sum by(area,instance)(jvm_memory_max_bytes)",
      "sum by(instance,id)(jvm_buffer_total_capacity_bytes)"
    ]
  },
  {
    "name": "Garbage Collection",
    "description": "",
    "queries": [
      "delta(jvm_gc_pause_seconds_sum[$__interval])",
      "delta(jvm_gc_pause_seconds_count[$__interval])"
    ]
  },
  {
    "name": "Threads",
    "description": "",
    "queries": [
      "sum by(instance)(jvm_threads_live_threads)",
      "sum by(instance)(jvm_threads_daemon_threads)",
      "sum by(instance)(jvm_threads_peak_threads)",
      "sum by(instance)(jvm_threads_states_threads)"
    ]
  },
  {
    "name": "JVM Classes",
    "description": "",
    "queries": [
      "jvm_classes_loaded_classes",
      "jvm_classes_unloaded_classes_total"
    ]
  }
]