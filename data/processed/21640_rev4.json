[
  {
    "name": "Active Workers $controller",
    "description": "",
    "queries": [
      "controller_runtime_active_workers"
    ]
  },
  {
    "name": "Max Concurrent Reconciles $controller",
    "description": "",
    "queries": [
      "controller_runtime_max_concurrent_reconciles"
    ]
  },
  {
    "name": "Reconcile Rate $controller",
    "description": "",
    "queries": [
      "sum by (controller, result) (rate(controller_runtime_reconcile_total[2m]))"
    ]
  },
  {
    "name": "Reconcile Errors Rate $controller",
    "description": "",
    "queries": [
      "sum by (controller) (rate(controller_runtime_reconcile_errors_total[2m]))"
    ]
  },
  {
    "name": "Runtime Recincile Time $controller",
    "description": "",
    "queries": [
      "histogram_quantile(\n  0.99,\n  sum by (le, controller)(\n    rate(\n      controller_runtime_reconcile_time_seconds_bucket[2m]\n    )\n  )\n)",
      "histogram_quantile(\n  0.95,\n  sum by (le, controller)(\n    rate(\n      controller_runtime_reconcile_time_seconds_bucket[2m]\n    )\n  )\n)",
      "histogram_quantile(\n  0.90,\n  sum by (le, controller)(\n    rate(\n      controller_runtime_reconcile_time_seconds_bucket[2m]\n    )\n  )\n)",
      "histogram_quantile(\n  0.50,\n  sum by (le, controller)(\n    rate(\n      controller_runtime_reconcile_time_seconds_bucket[2m]\n    )\n  )\n)"
    ]
  },
