[
  {
    "name": "Total Requests",
    "description": "",
    "queries": [
      "sum(http_server_requests_seconds_count)"
    ]
  },
  {
    "name": "Requests Count",
    "description": "",
    "queries": [
      "sum by(uri) (http_server_requests_seconds_count)"
    ]
  },
  {
    "name": "Requests Average Duration",
    "description": "",
    "queries": [
      "sum by(uri)(http_server_requests_seconds_sum) / sum by(uri)(http_server_requests_seconds_count)"
    ]
  },
  {
    "name": "Total Exceptions",
    "description": "",
    "queries": [
      "sum(http_server_requests_seconds_count)"
    ]
  },
  {
    "name": "Percent of 2xx Requests",
    "description": "",
    "queries": [
      "sum by(uri) (http_server_requests_seconds_count) / sum by(uri) (http_server_requests_seconds_count)"
    ]
  },
  {
    "name": "Percent of 5xx Requests",
    "description": "",
    "queries": [
      "sum by(uri) (http_server_requests_seconds_count) / sum by(uri) (http_server_requests_seconds_count)"
    ]
  },
  {
    "name": "PR 99 Requests Duration",
    "description": "",
    "queries": [
      "histogram_quantile(.99,sum(rate(http_server_requests_seconds_bucket[1m])) by(uri, le))"
    ]
  },
  {
    "name": "PR 95 Requests Duration",
    "description": "",
    "queries": [
      "histogram_quantile(.95,sum(rate(http_server_requests_seconds_bucket[1m])) by(uri, le))"
    ]
  },
  {
    "name": "Request Per Sec",
    "description": "",
    "queries": [
      "sum by(uri)(rate(http_server_requests_seconds_count[1m]))"
    ]
  },
  {
    "name": "Log Type Rate",
    "description": "",
    "queries": [
      "sum by(type) (rate( | pattern `<date> <time> <_>=<trace_id> <_>=<span_id> <_>=<trace_flags> <type> <_> --- <msg>` | type != \"\" |= \"$log_keyword\" [1m]))"
    ]
  },
  {
    "name": "Log of All Spring Boot Apps",
    "description": "",
    "queries": [
      " | pattern `<date> <time> <_>=<trace_id> <_>=<span_id> <_>=<trace_flags> <type> <_> --- <msg>` | line_format \"}\\t}\\ttrace_id=}\\t}\" |= \"$log_keyword\""
    ]
  }
]