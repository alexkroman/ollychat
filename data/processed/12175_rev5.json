[
  {
    "name": "Active hosts on each node",
    "description": "These metrics are part of general information related to your Calico implementation.",
    "queries": [
      "felix_active_local_endpoints"
    ]
  },
  {
    "name": "iptables save errors",
    "description": "",
    "queries": [
      "sum(rate(felix_iptables_save_errors[5m]))"
    ]
  },
  {
    "name": "ipset errors",
    "description": "",
    "queries": [
      "sum(rate(felix_ipset_errors[5m]))"
    ]
  },
  {
    "name": "Active calico nodes",
    "description": "",
    "queries": [
      "max(felix_cluster_num_hosts)"
    ]
  },
  {
    "name": "Errors plot",
    "description": "This graph shows you all the errors that Calico encounters, it is important to note occasional errors are acceptable. However, rise in the number of error or constant error counters means Calico is not working properly.",
    "queries": [
      "rate(felix_ipset_errors[5m])",
      "rate(felix_iptables_restore_errors[5m])",
      "rate(felix_iptables_save_errors[5m])",
      "rate(felix_log_errors[5m])"
    ]
  },
  {
    "name": "Felix cluster policies",
    "description": "More policies on Felix means more effort required by Calico to manage packets. ",
    "queries": [
      "felix_cluster_num_policies"
    ]
  },
  {
    "name": "iptables restore errors",
    "description": "",
    "queries": [
      "sum(rate(felix_iptables_restore_errors[5m]))"
    ]
  },
  {
    "name": "Felix log errors",
    "description": "",
    "queries": [
      "sum(rate(felix_log_errors[5m]))"
    ]
  },
  {
    "name": "Felix resync started",
    "description": "",
    "queries": [
      "sum(rate(felix_resyncs_started[5m])) "
    ]
  },
  {
    "name": "Felix dropped logs",
    "description": "",
    "queries": [
      "felix_logs_dropped"
    ]
  },
  {
    "name": "Dataplane apply time quantile 0.5",
    "description": "Dataplane apply time can indicate how busy your Kubernetes instance is. This can slow down Calico performance",
