[
  {
    "name": "Fluent-bit",
    "description": "",
    "queries": [
      "sum(kube_pod_info)"
    ]
  },
  {
    "name": "Fluentd",
    "description": "",
    "queries": [
      "sum(kube_pod_info)"
    ]
  },
  {
    "name": "Fluentbit Input Record",
    "description": "",
    "queries": [
      "sum(rate(fluentbit_input_records_total[1m]))"
    ]
  },
  {
    "name": "Fluentbit Output Records",
    "description": "",
    "queries": [
      "sum(rate(fluentbit_output_proc_records_total[1m]))"
    ]
  },
  {
    "name": "Fluentd Router records/s",
    "description": "",
    "queries": [
      "sum(rate(fluentd_router_records_total[1m]))"
    ]
  },
  {
    "name": "CPU Utilisation",
    "description": "",
    "queries": [
      "1 - avg(rate(node_cpu_seconds_total[1m]))"
    ]
  },
  {
    "name": "CPU Requests Commitment",
    "description": "",
    "queries": [
      "sum(kube_pod_container_resource_requests_cpu_cores) / sum(kube_node_status_allocatable_cpu_cores)"
    ]
  },
  {
    "name": "CPU Limits Commitment",
    "description": "",
    "queries": [
      "sum(kube_pod_container_resource_limits_cpu_cores) / sum(kube_node_status_allocatable_cpu_cores)"
    ]
  },
  {
    "name": "Ready Nodes",
    "description": "",
    "queries": [
      "sum(kube_node_status_condition)"
    ]
  },
  {
    "name": "Bad Nodes",
    "description": "",
    "queries": [
      "sum(kube_node_status_condition)"
    ]
  },
  {
    "name": "Fluentbit Input Bytes",
    "description": "",
    "queries": [
      "sum(rate(fluentbit_input_bytes_total[5m]))"
    ]
  },
  {
    "name": "Fluentbit Output Bytes",
    "description": "",
    "queries": [
      "sum(rate(fluentbit_output_proc_bytes_total[5m]))"
    ]
  },
  {
    "name": "Fluentd Output Records",
    "description": "",
    "queries": [
      "sum(rate(fluentd_output_status_emit_records[5m]))"
    ]
  },
  {
    "name": "Memory Utilisation",
    "description": "",
    "queries": [
      "1 - sum(:node_memory_MemAvailable_bytes:sum) / sum(kube_node_status_allocatable_memory_bytes)"
    ]
  },
  {
    "name": "Memory Requests Commitment",
    "description": "",
    "queries": [
      "sum(kube_pod_container_resource_requests_memory_bytes) / sum(kube_node_status_allocatable_memory_bytes)"
    ]
  },
  {
    "name": "Memory Limits Commitment",
    "description": "",
    "queries": [
      "sum(kube_pod_container_resource_limits_memory_bytes) / sum(kube_node_status_allocatable_memory_bytes)"
    ]
  },
  {
    "name": "Fluentd Records by Plugin",
    "description": "",
    "queries": [
      "rate(fluentd_output_status_emit_count[5m])"
    ]
  },
  {
    "name": "Top Aggregated Fluentd Routes",
    "description": "",
    "queries": [
      "topk(5, sum(rate(fluentd_router_records_total[1m])) by (flow, id))"
    ]
  },
  {
    "name": "Fluentd Output Buffer",
    "description": "",
    "queries": [
      "fluentd_output_status_buffer_total_bytes",
      "fluentd_output_status_buffer_queue_length"
    ]
  },
  {
    "name": "Fluentd Buffer Disk",
    "description": "",
    "queries": [
      "node_filesystem_size_bytes - node_filesystem_free_bytes",
      "node_filesystem_size_bytes"
    ]
  },
  {
    "name": "Fluentd Output Error and Retry",
    "description": "",
    "queries": [
      "sum(rate(fluentd_output_status_retry_count[1m])) by (pod)",
      "sum(rate(fluentd_output_status_num_errors[1m])) by (pod)"
    ]
  },
  {
    "name": "Fluentbit Input Bytes",
    "description": "",
    "queries": [
      "sum(rate(fluentbit_input_bytes_total[5m])) by (pod, name)"
    ]
  },
  {
    "name": "Fluentbit Output Bytes",
    "description": "",
    "queries": [
      "sum(rate(fluentbit_output_proc_bytes_total[5m])) by (pod, name)"
    ]
  },
  {
    "name": "Fluentbit Input Record",
    "description": "",
    "queries": [
      "sum(rate(fluentbit_input_records_total[5m])) by (pod, name)"
    ]
  },
  {
    "name": "Fluentbit Output Records",
    "description": "",
    "queries": [
      "sum(rate(fluentbit_output_proc_records_total[5m])) by (pod, name)"
    ]
  },
  {
    "name": "Fluentbit Output Retries/Fails",
    "description": "",
    "queries": [
      "sum(rate(fluentbit_output_retries_total[1m])) by (pod, name)",
      "sum(rate(fluentbit_output_retries_failed_total[1m])) by (pod, name)"
    ]
  },
  {
    "name": "Fluentbit Output Errors",
    "description": "",
    "queries": [
      "sum(rate(fluentbit_output_errors_total[1m])) by (pod, name)"
    ]
  },
  {
    "name": "Fluentbit Output Dropped Records",
    "description": "",
    "queries": [
      "sum(rate(fluentbit_output_dropped_records_total[5m])) by (pod, name)"
    ]
  },
  {
    "name": "Fluentbit Output Retried Records",
    "description": "",
    "queries": [
      "sum(rate(fluentbit_output_retried_records_total[5m])) by (pod, name)"
    ]
  }
