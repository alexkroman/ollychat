[
  {
    "name": "$service return code",
    "description": "",
    "queries": [
      "traefik_service_requests_total{service=\"$service\"}"
    ]
  },
  {
    "name": "$service response time",
    "description": "",
    "queries": [
      "sum(traefik_service_request_duration_seconds_sum{service=\"$service\"}) / sum(traefik_service_requests_total{service=\"$service\"}) * 1000"
    ]
  },
  {
    "name": "Total requests over 5min $service",
    "description": "",
    "queries": [
      "sum(rate(traefik_service_requests_total{service=\"$service\"}[5m]))"
    ]
  },
  {
    "name": "Status code 200 over 5min",
    "description": "",
    "queries": [
      "rate(traefik_entrypoint_requests_total{entrypoint=~\"$entrypoint\",code=\"200\"}[5m])"
    ]
  },
  {
    "name": "Others status code over 5min",
    "description": "",
    "queries": [
      "rate(traefik_entrypoint_requests_total{entrypoint=~\"$entrypoint\",code!=\"200\"}[5m])"
    ]
  },
  {
    "name": "Requests by service",
    "description": "",
    "queries": [
      "sum(rate(traefik_service_requests_total[5m])) by (service) "
    ]
  },
  {
    "name": "Requests by protocol",
    "description": "",
    "queries": [
      "sum(rate(traefik_entrypoint_requests_total{entrypoint =~ \"$entrypoint\"}[5m])) by (entrypoint) "
    ]
  }
]