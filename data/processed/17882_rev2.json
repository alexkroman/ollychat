[
  {
    "name": "Events Details",
    "description": "",
    "queries": [
      " |~ `(?i)$contains` | json | __error__=`` | metadata_namespace=~`$namespace`"
    ]
  },
  {
    "name": "Overview",
    "description": "",
    "queries": [
      "sum by (type) (count_over_time( | json | __error__=`` | metadata_namespace=~`$namespace` [$__interval]))"
    ]
  },
  {
    "name": "Warnings",
    "description": "",
    "queries": [
      "sum(count_over_time( | json | __error__=`` | metadata_namespace=~`$namespace` | type=\"Warning\" [$__interval]))"
    ]
  },
  {
    "name": "Image Pull Failed",
    "description": "",
    "queries": [
      "sum(count_over_time( |= \"Error: ImagePullBackOff\" | json | __error__=`` | metadata_namespace=~`$namespace` | reason=\"Failed\"  | line_format \"}\" [$__interval]))"
    ]
  },
  {
    "name": "Liveness Probe Failed",
    "description": "",
    "queries": [
      "sum(count_over_time( |= \"failed liveness probe\" | json | __error__=`` | metadata_namespace=~`$namespace` | reason=\"Killing\" [$__interval]))"
    ]
  },
  {
    "name": "Volume Mount Failed",
    "description": "",
    "queries": [
      "sum(count_over_time( | json | __error__=`` | metadata_namespace=~`$namespace` | reason=\"FailedMount\" [$__interval]))"
    ]
  },
  {
    "name": "Scheduling Failed",
    "description": "",
    "queries": [
      "sum(count_over_time( | json | __error__=`` | metadata_namespace=~`$namespace` | reason=\"FailedScheduling\" [$__interval]))"
    ]
  },
  {
    "name": "Container OOM Killed",
    "description": "",
    "queries": [
      "sum(count_over_time( | json | __error__=`` | metadata_namespace=~`$namespace` | reason=\"OOMKilling\" [$__interval]))"
    ]
  },
  {
    "name": "Container Crashed",
    "description": "",
    "queries": [
      "sum(count_over_time( | json | __error__=`` | metadata_namespace=~`$namespace` | reason=\"BackOff\" [$__interval]))"
    ]
  },
  {
    "name": "Pod Evicted",
    "description": "",
    "queries": [
      "sum(count_over_time( |= \"Marking for deletion Pod\" | json | __error__=`` | metadata_namespace=~`$namespace` | reason=\"TaintManagerEviction\" [$__interval]))"
    ]
  },
  {
    "name": "System OOM",
    "description": "",
    "queries": [
      "sum(count_over_time( | json | __error__=`` | metadata_namespace=~`$namespace` | reason=\"SystemOOM\" [$__interval]))"
    ]
  }
]