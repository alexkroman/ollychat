[
  {
    "name": "Spans Accepted vs Refused /Second",
    "description": "Rate of spans successfully accepted vs refused per second",
    "queries": [
      "sum(rate(otelcol_receiver_accepted_spans[1m])) by (receiver)",
      "sum(rate(otelcol_receiver_refused_spans[1m])) by (receiver)"
    ]
  },
  {
    "name": "Metrics Accepted vs Refused Received/Second",
    "description": "Rate of metrics successfully accepted vs refused per second",
    "queries": [
      "sum(rate(otelcol_receiver_accepted_metric_points[30m])) by (receiver)",
      "sum(rate(otelcol_receiver_refused_metric_points[30m])) by (receiver)"
    ]
  },
  {
    "name": "Logs Accepted vs Refused Received/Second",
    "description": "Rate of metrics successfully accepted vs refused per second",
    "queries": [
      "sum(rate(otelcol_receiver_accepted_log_records[1m])) by (receiver)",
      "sum(rate(otelcol_receiver_refused_metric_points[1m])) by (receiver)"
    ]
  },
  {
    "name": "Spans Exported vs Failed / Second",
    "description": "Rate of spans successfully exported vs Failed per second",
    "queries": [
      "sum(rate(otelcol_exporter_sent_spans[1m])) by (exporter)",
      "sum(rate(otelcol_exporter_send_failed_spans[1m])) by (exporter)",
      "sum(rate(otelcol_exporter_enqueue_failed_spans[1m])) by (exporter)"
    ]
  },
  {
    "name": "Metrics Exported vs failed /Second",
    "description": "Rate of timeseries successfully exported per second",
    "queries": [
      "sum(rate(otelcol_exporter_sent_metric_points[30m])) by (exporter) > 0",
      "sum(rate(otelcol_exporter_send_failed_metric_points[30m])) by (exporter) >0",
      "sum(rate(otelcol_exporter_enqueue_failed_metric_points[30m])) by (exporter) >0"
    ]
  },
  {
    "name": "Logs Exported vs failed /Second",
    "description": "Rate of timeseries successfully exported per second",
    "queries": [
      "sum(rate(otelcol_exporter_sent_log_records[1m])) by (exporter)",
      "sum(rate(otelcol_exporter_send_failed_log_records[1m])) by (exporter)",
      "sum(rate(otelcol_exporter_enqueue_failed_log_records[1m])) by (exporter)"
    ]
  },
  {
    "name": "Exporter Queue Size",
    "description": "",
    "queries": [
      "max(otelcol_exporter_queue_capacity) by(exporter)",
      "max(otelcol_exporter_queue_size) by(exporter)"
    ]
  },
  {
    "name": "Failed Request Rate",
    "description": "",
    "queries": [
      "sum(rate(otelcol_exporter_send_failed_requests[30m]))"
    ]
  },
  {
    "name": "CPU Usage",
    "description": "",
    "queries": [
      "max(rate(otelcol_process_cpu_seconds[30m])*100) by (instance)",
      "avg(rate(otelcol_process_cpu_seconds[30m])*100) by (instance)",
      "min(rate(otelcol_process_cpu_seconds[30m])*100) by (instance)"
    ]
  },
  {
    "name": "Memory RSS",
    "description": "",
    "queries": [
      "max(otelcol_process_memory_rss)",
      "avg(otelcol_process_memory_rss)",
      "min(otelcol_process_memory_rss)"
    ]
  }
]