      "histogram_quantile(0.5, sum(rate(workqueue_queue_duration_seconds_bucket[5m])) by (le))"
    ]
  },
  {
    "name": "apiserver request latency",
    "description": "95th percentile request latency for Kubernetes apiserver component",
    "queries": [
      "histogram_quantile(0.95, sum(rate(apiserver_request_duration_seconds_bucket[5m])) by (le))",
      "histogram_quantile(0.90, sum(rate(apiserver_request_duration_seconds_bucket[5m])) by (le))",
      "histogram_quantile(0.5, sum(rate(apiserver_request_duration_seconds_bucket[5m])) by (le))"
    ]
  },
  {
    "name": "workqueue processing time",
    "description": "",
    "queries": [
      "histogram_quantile(0.95, sum(rate(workqueue_work_duration_seconds_bucket[5m])) by (le))",
