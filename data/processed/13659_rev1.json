[
  {
    "name": "HTTP Probe Overview",
    "description": "",
    "queries": [
      "probe_success",
      "probe_http_ssl > 0",
      "(probe_ssl_earliest_cert_expiry - time()) / 3600 / 24",
      "probe_http_status_code > 0",
      "avg_over_time(probe_duration_seconds[1m])",
      "probe_tls_version_info",
      "avg_over_time(probe_dns_lookup_time_seconds[1m])"
    ]
  },
  {
    "name": "HTTP Probe Duration",
    "description": "Returns how long the probe took to complete in seconds",
    "queries": [
      "sum(probe_http_duration_seconds) by (instance)"
    ]
  }
]