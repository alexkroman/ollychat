[
  {
    "name": "CPU",
    "description": "",
    "queries": [
      "avg(irate(container_cpu_usage_seconds_total[5m]) * 100) by (pod_name)"
    ]
  },
  {
    "name": "Memory",
    "description": "",
    "queries": [
      "sum (container_memory_working_set_bytes) by (pod_name)"
    ]
  },
  {
    "name": "Nginx Upstream Response time (msec)",
    "description": "nginx_upstream_response_msecs_avg",
    "queries": [
      "nginx_upstream_response_msecs_avg"
    ]
  },
  {
    "name": "nginx_connections_total - 1m",
    "description": "",
    "queries": [
      "sum by (type) (irate(nginx_connections_total[1m]) )"
    ]
  },
  {
    "name": "Network RX",
    "description": "",
    "queries": [
      "avg(irate(container_network_receive_bytes_total[5m])) by (pod_name)"
    ]
  },
  {
    "name": "Network TX",
    "description": "",
    "queries": [
      "avg(irate(container_network_transmit_bytes_total[5m])) by (pod_name)"
    ]
  }
]