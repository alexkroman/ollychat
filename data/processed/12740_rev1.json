[
  {
    "name": "Network I/O pressure",
    "description": "",
    "queries": [
      "sum (rate (container_network_receive_bytes_total[1m]))",
      "- sum (rate (container_network_transmit_bytes_total[1m]))"
    ]
  },
  {
    "name": "Cluster memory usage",
    "description": "",
    "queries": [
      "sum (container_memory_working_set_bytes) / sum (machine_memory_bytes) * 100"
    ]
  },
  {
    "name": "Cluster CPU usage (1m avg)",
    "description": "",
    "queries": [
      "sum (rate (container_cpu_usage_seconds_total[1m])) / sum (machine_cpu_cores) * 100"
    ]
  },
  {
    "name": "Cluster filesystem usage",
    "description": "",
    "queries": [
      "sum (container_fs_usage_bytes) / sum (container_fs_limit_bytes) * 100"
    ]
  },
  {
    "name": "Used",
