[
  {
    "name": "$service Process start time",
    "description": "Nodejs runtime metrics",
    "query": "(process_start_time_seconds)*1000",
    "metrics": [
      "process_start_time_seconds"
    ]
  },
  {
    "name": "$service Max file descriptors",
    "description": "Nodejs runtime metrics",
    "query": "process_max_fds",
    "metrics": [
      "process_max_fds"
    ]
  },
  {
    "name": "$service Open file descriptors",
    "description": "Nodejs runtime metrics",
    "query": "process_open_fds",
    "metrics": [
      "process_open_fds"
    ]
  },
  {
    "name": "% CPU usage",
    "description": "Nodejs runtime metrics",
    "query": "rate(process_cpu_seconds_total[$interval])",
    "metrics": [
      "process_cpu_seconds_total"
    ]
  },
  {
    "name": "Process Memory",
    "description": "Nodejs runtime metrics",
    "query": "process_resident_memory_bytes",
    "metrics": [
      "process_resident_memory_bytes"
    ]
  },
  {
    "name": "Process Memory",
    "description": "Nodejs runtime metrics",
    "query": "process_virtual_memory_bytes",
    "metrics": [
      "process_virtual_memory_bytes"
    ]
  }
]