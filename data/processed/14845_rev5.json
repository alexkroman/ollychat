[
  {
    "name": "JDK",
    "description": "",
    "queries": [
      "label_join(jvm_info, \"jdk\", \", \", \"vendor\", \"runtime\", \"version\")"
    ]
  },
  {
    "name": "Status",
    "description": "",
    "queries": [
      "up"
    ]
  },
  {
    "name": "State",
    "description": "",
    "queries": [
      "up"
    ]
  },
  {
    "name": "Uptime",
    "description": "",
    "queries": [
      "java_lang_Runtime_Uptime"
    ]
  },
  {
    "name": "Start time",
    "description": "",
    "queries": [
      "java_lang_Runtime_StartTime"
    ]
  },
  {
    "name": "CPU Cores",
    "description": "",
    "queries": [
      "java_lang_OperatingSystem_AvailableProcessors"
    ]
  },
  {
    "name": "Total RAM",
    "description": "",
    "queries": [
      "java_lang_OperatingSystem_TotalPhysicalMemorySize"
    ]
  },
  {
    "name": "Total SWAP",
    "description": "",
    "queries": [
      "java_lang_OperatingSystem_TotalSwapSpaceSize"
    ]
  },
  {
    "name": "Open file descriptors",
    "description": "",
    "queries": [
      "java_lang_OperatingSystem_OpenFileDescriptorCount"
    ]
  },
  {
    "name": "Cpu Time",
    "description": "",
    "queries": [
      "java_lang_OperatingSystem_ProcessCpuTime",
      "java_lang_Threading_CurrentThreadCpuTime",
      "process_cpu_seconds_total"
    ]
  },
  {
    "name": "CPU System load average",
    "description": "",
    "queries": [
      "java_lang_OperatingSystem_SystemCpuLoad * 100"
    ]
  },
  {
    "name": "CPU Process Load (1m avg)",
    "description": "",
    "queries": [
      "avg_over_time(java_lang_OperatingSystem_ProcessCpuLoad[1m]) * 100"
    ]
  },
  {
    "name": "CPU System Load (1m avg)",
    "description": "",
    "queries": [
      "avg_over_time(java_lang_OperatingSystem_SystemCpuLoad[1m]) * 100"
    ]
  },
  {
    "name": "Memory Utilization",
    "description": "",
    "queries": [
      "100 - (100 * java_lang_OperatingSystem_FreePhysicalMemorySize / on(instance) java_lang_OperatingSystem_TotalPhysicalMemorySize)"
    ]
  },
  {
    "name": "SWAP Utilization",
    "description": "",
    "queries": [
      "100 - (java_lang_OperatingSystem_FreeSwapSpaceSize / on(instance) java_lang_OperatingSystem_TotalSwapSpaceSize * 100)"
    ]
  },
  {
    "name": "CPU load",
    "description": "",
    "queries": [
      "java_lang_OperatingSystem_SystemCpuLoad",
      "java_lang_OperatingSystem_ProcessCpuLoad"
    ]
  },
  {
    "name": "Open File Descriptors",
    "description": "",
    "queries": [
      "process_open_fds"
    ]
  },
  {
    "name": "Physical memory",
    "description": "",
    "queries": [
      "java_lang_OperatingSystem_FreePhysicalMemorySize",
      "java_lang_OperatingSystem_TotalPhysicalMemorySize",
      "java_lang_OperatingSystem_CommittedVirtualMemorySize"
    ]
  },
  {
    "name": "Process Memory",
    "description": "",
    "queries": [
      "process_resident_memory_bytes",
      "process_virtual_memory_bytes"
    ]
  },
  {
    "name": "JVM Memory Used [$memarea]",
    "description": "",
    "queries": [
      "jvm_memory_bytes_used",
      "jvm_memory_bytes_max"
    ]
  },
  {
    "name": "JVM Memory Usage [$memarea]",
    "description": "",
    "queries": [
      "jvm_memory_bytes_used / ignoring(area) group_left java_lang_OperatingSystem_TotalPhysicalMemorySize *100",
      "jvm_memory_bytes_used / (jvm_memory_bytes_max >=0) *100"
    ]
  },
  {
    "name": "JVM Memory committed",
    "description": "",
    "queries": [
      "jvm_memory_bytes_committed"
    ]
  },
  {
    "name": "Memory pool [$mempool]",
    "description": "",
    "queries": [
      "jvm_memory_pool_bytes_max",
      "jvm_memory_pool_bytes_used",
      "jvm_memory_pool_bytes_committed",
      "rate(jvm_memory_pool_bytes_committed[30m])"
    ]
  },
  {
    "name": "GC count increase",
    "description": "",
    "queries": [
      "increase(jvm_gc_collection_seconds_count[$__interval])"
    ]
  },
  {
    "name": "GC count rate",
    "description": "",
    "queries": [
      "rate(jvm_gc_collection_seconds_count[30m])"
    ]
  },
  {
    "name": "GC Time increase",
    "description": "",
    "queries": [
      "increase(jvm_gc_collection_seconds_sum[$__interval])"
    ]
  },
  {
    "name": "GC Time rate",
    "description": "",
    "queries": [
      "rate(jvm_gc_collection_seconds_sum[30m])"
    ]
  },
  {
    "name": "JVM Threading Threads",
    "description": "",
    "queries": [
      "java_lang_Threading_ThreadCount",
      "java_lang_Threading_DaemonThreadCount",
      "java_lang_Threading_PeakThreadCount",
      "java_lang_Threading_TotalStartedThreadCount"
    ]
  },
  {
    "name": "JVM Threads current",
    "description": "",
    "queries": [
      "jvm_threads_current",
      "jvm_threads_daemon",
      "jvm_threads_deadlocked",
      "jvm_threads_deadlocked_monitor",
      "jvm_threads_peak",
      "jvm_threads_started_total"
    ]
  },
  {
    "name": "JVM Threading Threads increase",
    "description": "",
    "queries": [
      "increase(java_lang_Threading_ThreadCount[$__interval])",
      "increase(java_lang_Threading_DaemonThreadCount[$__interval])",
      "increase(java_lang_Threading_PeakThreadCount[$__interval])",
      "increase(java_lang_Threading_TotalStartedThreadCount[$__interval])"
    ]
  },
  {
    "name": "JVM Threads current increase",
    "description": "",
    "queries": [
      "increase(jvm_threads_current[$__interval])",
      "increase(jvm_threads_daemon[$__interval])",
      "increase(jvm_threads_deadlocked[$__interval])",
      "increase(jvm_threads_deadlocked_monitor[$__interval])",
      "increase(jvm_threads_peak[$__interval])",
      "increase(jvm_threads_started_total[$__interval])"
    ]
  },
  {
    "name": "JVM Threading Threads rate",
    "description": "",
    "queries": [
      "rate(java_lang_Threading_ThreadCount[30m])",
      "rate(java_lang_Threading_DaemonThreadCount[30m])",
      "rate(java_lang_Threading_PeakThreadCount[30m])",
      "rate(java_lang_Threading_TotalStartedThreadCount[30m])"
    ]
  },
  {
    "name": "JVM Threads current rate",
    "description": "",
    "queries": [
      "increase(jvm_threads_current[30m])",
      "rate(jvm_threads_daemon[30m])",
      "rate(jvm_threads_deadlocked[30m])",
      "rate(jvm_threads_deadlocked_monitor[30m])",
      "rate(jvm_threads_peak[30m])",
      "rate(jvm_threads_started_total[30m])"
    ]
  },
  {
    "name": "Class Loading count",
    "description": "",
    "queries": [
      "java_lang_ClassLoading_LoadedClassCount",
      "java_lang_ClassLoading_UnloadedClassCount"
    ]
  },
  {
    "name": "Classes Loaded",
    "description": "",
    "queries": [
      "jvm_classes_currently_loaded ",
      "jvm_classes_loaded_total"
    ]
  },
  {
    "name": "Class Loading count increase",
    "description": "",
    "queries": [
      "increase(java_lang_ClassLoading_LoadedClassCount[$__interval])",
      "increase(java_lang_ClassLoading_UnloadedClassCount[$__interval])"
    ]
  },
  {
    "name": "Classes Loaded increase",
    "description": "",
    "queries": [
      "increase(jvm_classes_currently_loaded [$__interval])",
      "increase(jvm_classes_loaded_total[$__interval])"
    ]
  },
  {
    "name": "Class Loading count rate",
    "description": "",
    "queries": [
      "rate(java_lang_ClassLoading_LoadedClassCount[30m])",
      "rate(java_lang_ClassLoading_UnloadedClassCount[30m])"
    ]
  },
  {
    "name": "Classes Loaded rate",
    "description": "",
    "queries": [
      "rate(jvm_classes_currently_loaded [30m])",
      "rate(jvm_classes_loaded_total[30m])"
    ]
  }
]