[
  {
    "name": "RAM Used",
    "description": "Non available RAM memory",
    "query": "((node_memory_MemTotal_bytes - node_memory_MemFree_bytes) / (node_memory_MemTotal_bytes )) * 100",
    "metrics": [
      "node_memory_MemFree_bytes",
      "node_memory_MemTotal_bytes"
    ]
  },
  {
    "name": "RAM Used",
    "description": "Non available RAM memory",
    "query": "100 - ((node_memory_MemAvailable_bytes * 100) / node_memory_MemTotal_bytes)",
    "metrics": [
      "node_memory_MemAvailable_bytes",
      "node_memory_MemTotal_bytes"
    ]
  },
  {
    "name": "SWAP Used",
    "description": "Used Swap",
    "query": "((node_memory_SwapTotal_bytes - node_memory_SwapFree_bytes) / (node_memory_SwapTotal_bytes )) * 100",
    "metrics": [
      "node_memory_SwapFree_bytes",
      "node_memory_SwapTotal_bytes"
    ]
  },
  {
    "name": "Root FS Used",
    "description": "Used Root FS",
    "query": "100 - ((node_filesystem_avail_bytes * 100) / node_filesystem_size_bytes)",
    "metrics": [
      "node_filesystem_avail_bytes",
      "node_filesystem_size_bytes"
    ]
  },
  {
    "name": "Uptime",
    "description": "System uptime",
    "query": "node_time_seconds - node_boot_time_seconds",
    "metrics": [
      "node_boot_time_seconds",
      "node_time_seconds"
    ]
  },
  {
    "name": "RootFS Total",
    "description": "Total RootFS",
    "query": "node_filesystem_size_bytes",
    "metrics": [
      "node_filesystem_size_bytes"
    ]
  },
  {
    "name": "RAM Total",
