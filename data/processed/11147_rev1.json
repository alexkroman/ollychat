[
  {
    "name": "",
    "description": "",
    "queries": [
      "node_openwrt_info"
    ]
  },
  {
    "name": "",
    "description": "",
    "queries": [
      "node_uname_info"
    ]
  },
  {
    "name": "CPU Busy",
    "description": "Busy state of all CPU cores together",
    "queries": [
      "(((count(count(node_cpu_seconds_total) by (cpu))) - avg(sum by (mode)(irate(node_cpu_seconds_total[5m])))) * 100) / count(count(node_cpu_seconds_total) by (cpu))"
    ]
  },
  {
    "name": "Used RAM Memory",
    "description": "Non available RAM memory",
    "queries": [
      "((node_memory_MemTotal_bytes - node_memory_MemFree_bytes) / (node_memory_MemTotal_bytes )) * 100",
      "100 - ((node_memory_MemAvailable_bytes * 100) / node_memory_MemTotal_bytes)"
    ]
  },
  {
    "name": "Wifi Quality",
    "description": "average Wifi quality",
    "queries": [
      "avg(wifi_network_quality)"
    ]
  },
  {
    "name": "CPU System Load (1m avg)",
    "description": "Busy state of all CPU cores together (1 min average)",
    "queries": [
      "avg(node_load1) /  count(count(node_cpu_seconds_total) by (cpu)) * 100"
    ]
  },
  {
    "name": "CPU System Load (5m avg)",
    "description": "Busy state of all CPU cores together (5 min average)",
    "queries": [
      "avg(node_load5) /  count(count(node_cpu_seconds_total) by (cpu)) * 100"
    ]
  },
  {
    "name": "Used SWAP",
    "description": "Used Swap",
    "queries": [
      "((node_memory_SwapTotal_bytes - node_memory_SwapFree_bytes) / (node_memory_SwapTotal_bytes )) * 100"
    ]
  },
  {
    "name": "Used Root FS",
    "description": "Used Root FS",
    "queries": [
      "100 - ((node_filesystem_avail_bytes * 100) / node_filesystem_size_bytes)"
    ]
  },
  {
    "name": "CPU Cores",
    "description": "Total number of CPU cores",
    "queries": [
      "count(count(node_cpu_seconds_total) by (cpu))"
    ]
  },
  {
    "name": "Wifi Clients",
    "description": "all connected Wifi clients",
    "queries": [
      "sum(wifi_stations)"
    ]
  },
  {
    "name": "Uptime",
    "description": "System uptime",
    "queries": [
      "node_time_seconds - node_boot_time_seconds"
    ]
  },
  {
    "name": "Total RAM",
    "description": "Total RAM",
    "queries": [
      "node_memory_MemTotal_bytes"
    ]
  },
  {
    "name": "Total Network Traffic",
    "description": "Total network traffic",
    "queries": [
      "(sum(node_network_transmit_bytes_total) + sum(node_network_receive_bytes_total))"
    ]
  },
  {
    "name": "Total NAT Traffic",
    "description": "Total NAT traffic (WAN)",
    "queries": [
      "sum(node_nat_traffic)"
    ]
  },
  {
    "name": "Total SWAP",
    "description": "Total SWAP",
    "queries": [
      "node_memory_SwapTotal_bytes"
    ]
  },
  {
    "name": "Total RootFS",
    "description": "Total RootFS",
    "queries": [
      "node_filesystem_size_bytes"
    ]
  },
  {
    "name": "Transmit Bytes Total",
    "description": "",
    "queries": [
      "node_network_transmit_bytes_total/1024/1024"
    ]
  },
  {
    "name": "Receive Bytes Total",
    "description": "",
    "queries": [
      "node_network_receive_bytes_total/1024/1024"
    ]
  },
  {
    "name": "Network Traffic by Bytes",
    "description": "",
    "queries": [
      "irate(node_network_receive_bytes_total[5m])/1024",
      "irate(node_network_transmit_bytes_total[5m])/1024"
    ]
  },
  {
    "name": "Network Traffic by Packets",
    "description": "",
    "queries": [
      "irate(node_network_transmit_packets_total[5m])",
      "irate(node_network_receive_packets_total[5m])"
    ]
  },
  {
    "name": "Transmit Packets Total",
    "description": "",
    "queries": [
      "node_network_transmit_packets_total"
    ]
  },
  {
    "name": "Receive Packets Total",
    "description": "",
    "queries": [
      "node_network_receive_packets_total"
    ]
  },
  {
    "name": "Top NAT Traffic",
    "description": "",
    "queries": [
      "topk(15, node_nat_traffic/1024/1024)"
    ]
  }
]