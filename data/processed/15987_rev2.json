[
  {
    "name": "Memory Utilisation (from requests)",
    "description": "",
    "query": "sum(container_memory_working_set_bytes) / sum(kube_pod_container_resource_requests)",
    "metrics": [
      "container_memory_working_set_bytes",
      "kube_pod_container_resource_requests"
    ]
  },
  {
    "name": "Memory Utilisation (from limits)",
    "description": "",
    "query": "sum(container_memory_working_set_bytes) / sum(kube_pod_container_resource_limits)",
    "metrics": [
      "container_memory_working_set_bytes",
      "kube_pod_container_resource_limits"
    ]
  },
  {
    "name": "Memory Usage (w/o cache)",
    "description": "",
    "query": "sum(container_memory_working_set_bytes) by (pod)",
    "metrics": [
      "container_memory_working_set_bytes"
    ]
  }
]