[
  {
    "name": "Current Memory HEAP",
    "description": "",
    "queries": [
      "sum(jvm_memory_bytes_used)*100/sum(jvm_memory_bytes_max)"
    ]
  },
  {
    "name": "Current Memory nonHEAP",
    "description": "",
    "queries": [
      "sum(jvm_memory_bytes_used)*100/sum(jvm_memory_bytes_max)"
    ]
  },
  {
    "name": "Memory Usage",
    "description": "",
    "queries": [
      "sum(jvm_memory_bytes_max)",
      "sum(jvm_memory_bytes_committed)",
      "sum(jvm_memory_bytes_used)"
    ]
  },
  {
    "name": "Logins Per REALM for past 24h",
