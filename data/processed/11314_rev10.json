  {
    "name": "5 GHz Traffic",
    "description": "RX is on the negative Axis.",
    "queries": [
      "sum (rate(unpoller_device_vap_transmit_bytes_total[5m])) by (name)",
      "sum (rate(unpoller_device_vap_receive_bytes_total[5m])) by (name)"
    ]
  },
  {
    "name": "2.4 GHz Traffic",
    "description": "RX is on the negative Axis.",
    "queries": [
      "sum (rate(unpoller_device_vap_transmit_bytes_total[5m])) by (name)",
      "sum (rate(unpoller_device_vap_receive_bytes_total[5m])) by (name)"
    ]
  },
  {
    "name": "5 GHz Packets",
    "description": "PPS on the na band. In is on the negative Axis.",
    "queries": [
      "sum (rate(unpoller_device_vap_transmit_packets_total[$__interval])) by (name)",
      "sum (rate(unpoller_device_vap_receive_packets_total[$__interval])) by (name)"
    ]
  },
  {
    "name": "2.4 GHz Packets",
    "description": "PPS on the ng band calculated in 30 second buckets. In is on the negative Axis.",
    "queries": [
      "sum (rate(unpoller_device_vap_transmit_packets_total[$__interval])) by (name)",
      "sum (rate(unpoller_device_vap_receive_packets_total[$__interval])) by (name)"
    ]
  },
  {
    "name": "5 GHz Drops/Errors",
    "description": "Visualize packet errors (several types)  per second in the 5GHz band.",
    "queries": [
      "sum (rate(unpoller_device_vap_receive_dropped_total[$__interval])) by (name)",
      "sum (rate(unpoller_device_vap_transmit_dropped_total[$__interval])) by (name)",
      "sum (rate(unpoller_device_vap_receive_errors_total[$__interval])) by (name)",
      "sum (rate(unpoller_device_vap_transmit_errors_total[$__interval])) by (name)",
      "sum (rate(unpoller_device_vap_receive_crypts_total[$__interval])) by (name)",
      "sum (rate(unpoller_device_vap_receive_frags_total[$__interval])) by (name)",
      "sum (rate(unpoller_device_vap_receive_nwids_total[$__interval])) by (name)",
      "sum (rate(unpoller_device_vap_transmit_retries_total[$__interval])) by (name)"
    ]
  },
  {
    "name": "2.4 GHz Drops/Errors",
    "description": "In is on the negative axis.",
    "queries": [
      "sum (rate(unpoller_device_vap_receive_dropped_total[$__interval])) by (name)",
      "sum (rate(unpoller_device_vap_transmit_dropped_total[$__interval])) by (name)",
      "sum (rate(unpoller_device_vap_receive_errors_total[$__interval])) by (name)",
      "sum (rate(unpoller_device_vap_transmit_errors_total[$__interval])) by (name)",
      "sum (rate(unpoller_device_vap_receive_crypts_total[$__interval])) by (name)",
      "sum (rate(unpoller_device_vap_receive_frags_total[$__interval])) by (name)",
      "sum (rate(unpoller_device_vap_receive_nwids_total[$__interval])) by (name)",
      "sum (rate(unpoller_device_vap_transmit_retries_total[$__interval])) by (name)"
