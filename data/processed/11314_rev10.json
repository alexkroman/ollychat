[
  {
    "name": "Clients / Channel",
    "description": "Visualize wireless channel usage w/ wired client counts.",
    "queries": [
      "count by (channel) (unpoller_client_roam_count_total)\n"
    ]
  },
  {
    "name": "Clients / AP Radio",
    "description": "Visualize how many clients are connected to which radios/APs.",
    "queries": [
      "count by (radio_desc) (unpoller_client_uptime_seconds)"
    ]
  },
  {
    "name": "Client / MAC OUI",
    "description": "This shows an OUI breakdown from Unifi's perspective.",
    "queries": [
      "count by (oui) (unpoller_client_uptime_seconds)"
    ]
  },
  {
    "name": "Users",
    "description": "",
    "queries": [
      "sum(unpoller_device_stations)"
    ]
  },
  {
    "name": "Guests",
    "description": "",
    "queries": [
      "sum(unpoller_device_stations)"
    ]
  },
  {
    "name": "Access Point Details",
    "description": "",
    "queries": [
      "sum by (ip,mac,model,name,serial,site_name,type,version) (unpoller_device_info)",
      "sum by (ip,mac,model,name,serial,site_name,type,version) (unpoller_device_uptime_seconds)",
      "sum by (ip,mac,model,name,serial,site_name,type,version) (unpoller_device_bytes_total)"
    ]
  },
  {
    "name": "Networks / UAP",
    "description": "The counters in this table represent uptime totals and do not change with the time range selector. ",
    "queries": [
      "sum by (bssid,essid,ap_name,name,radio,radio_name,vap_name,site_name,usage,source) (unpoller_device_vap_ccq_ratio)",
      "sum by (bssid,essid,ap_name,name,radio,radio_name,vap_name,site_name,usage,source) (unpoller_device_vap_average_client_signal)",
      "sum by (bssid,essid,ap_name,name,radio,radio_name,vap_name,site_name,usage,source) (unpoller_device_vap_receive_dropped_total) ",
      "sum by (bssid,essid,ap_name,name,radio,radio_name,vap_name,site_name,usage,source) (unpoller_device_vap_transmit_dropped_total) ",
      "sum by (bssid,essid,ap_name,name,radio,radio_name,vap_name,site_name,usage,source) (unpoller_device_vap_transmit_bytes_total) ",
      "sum by (bssid,essid,ap_name,name,radio,radio_name,vap_name,site_name,usage,source) (unpoller_device_vap_receive_bytes_total) ",
      "sum by (bssid,essid,ap_name,name,radio,radio_name,vap_name,site_name,usage,source) (unpoller_device_vap_receive_packets_total) ",
      "sum by (bssid,essid,ap_name,name,radio,radio_name,vap_name,site_name,usage,source) (unpoller_device_vap_transmit_packets_total) "
    ]
  },
